{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "expensetrackr/expensetrackr.app",
    "type": "project",
    "description": "A comprehensive web application for effortless expense management, supporting both personal and business finance tracking. Features include account organization, transaction categorization, team collaboration, insightful graphics, and financial reporting.",
    "keywords": [
        "expense-tracker",
        "finance-management",
        "budget-planner",
        "personal-finance",
        "business-finance",
        "financial-reporting",
        "team-collaboration",
        "laravel",
        "web-application",
        "financial-analytics"
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.3",
        "ext-redis": "*",
        "afatmustafa/blade-hugeicons": "1.0.2",
        "akaunting/laravel-money": "6.0.2",
        "cjmellor/browser-sessions": "1.3.1",
        "danestves/laravel-polar": "1.2.1",
        "filament/filament": "3.3.26",
        "flowframe/laravel-trend": "0.4.0",
        "http-interop/http-factory-guzzle": "1.2.0",
        "inertiajs/inertia-laravel": "2.0.2",
        "intervention/image-laravel": "1.5.6",
        "joelbutcher/socialstream": "6.3.0",
        "kongulov/interact-with-enum": "1.1.0",
        "laravel/fortify": "1.26.0",
        "laravel/framework": "12.18.0",
        "laravel/horizon": "5.33.0",
        "laravel/octane": "2.9.3",
        "laravel/pulse": "1.4.2",
        "laravel/sanctum": "4.1.1",
        "laravel/scout": "10.15.0",
        "laravel/tinker": "2.10.1",
        "laravel/wayfinder": "0.1.4",
        "league/flysystem-aws-s3-v3": "3.29.0",
        "lorisleiva/laravel-actions": "2.9.0",
        "meilisearch/meilisearch-php": "1.15.0",
        "nunomaduro/essentials": "0.1.0",
        "pinkary-project/type-guard": "0.1.0",
        "resend/resend-laravel": "0.19.0",
        "sentry/sentry-laravel": "4.15.0",
        "spatie/cpu-load-health-check": "1.0.4",
        "spatie/enum": "3.13.0",
        "spatie/laravel-data": "4.15.2",
        "spatie/laravel-health": "1.34.3",
        "spatie/laravel-permission": "6.20.0",
        "spatie/laravel-prefixed-ids": "1.4.1",
        "spatie/laravel-query-builder": "6.3.2",
        "spatie/laravel-sitemap": "7.3.6",
        "spatie/laravel-typescript-transformer": "2.5.2",
        "spatie/security-advisories-health-check": "1.2.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "3.5.5",
        "fakerphp/faker": "1.24.1",
        "larastan/larastan": "3.4.2",
        "laravel/pail": "1.2.3",
        "laravel/pint": "1.22.1",
        "laravel/sail": "1.43.1",
        "mockery/mockery": "1.6.12",
        "nunomaduro/collision": "8.8.1",
        "peckphp/peck": "0.1.3",
        "pestphp/pest": "3.8.2",
        "pestphp/pest-plugin-drift": "3.0.0",
        "pestphp/pest-plugin-laravel": "3.2.0",
        "pestphp/pest-plugin-type-coverage": "3.5.1",
        "rector/rector": "2.0.18",
        "soloterm/solo": "0.5.0",
        "wulfheart/laravel-actions-ide-helper": "0.10.0"
    },
    "autoload": {
        "files": [
            "bootstrap/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "php artisan solo"
        ],
        "lint": "pint",
        "refactor": "rector",
        "test:lint": "pint --test",
        "test:typos": "peck",
        "test:refactor": "rector --dry-run",
        "test:types": "phpstan analyse",
        "test:arch": "pest --filter=arch",
        "test:type-coverage": "pest --type-coverage --min=100",
        "test:unit": "pest --parallel --coverage --exactly=99.5",
        "test": [
            "@test:lint",
            "@test:typos",
            "@test:refactor",
            "@test:types",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
