{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "expensetrackr/expensetrackr.app",
    "type": "project",
    "description": "A comprehensive web application for effortless expense management, supporting both personal and business finance tracking. Features include account organization, transaction categorization, team collaboration, insightful graphics, and financial reporting.",
    "keywords": [
        "expense-tracker",
        "finance-management",
        "budget-planner",
        "personal-finance",
        "business-finance",
        "financial-reporting",
        "team-collaboration",
        "laravel",
        "web-application",
        "financial-analytics"
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.3",
        "akaunting/laravel-money": "6.0.2",
        "cjmellor/browser-sessions": "1.3.1",
        "danestves/laravel-polar": "1.1.1",
        "filament/filament": "3.3.10",
        "http-interop/http-factory-guzzle": "1.2.0",
        "inertiajs/inertia-laravel": "2.0.1",
        "intervention/image-laravel": "1.5.6",
        "joelbutcher/socialstream": "6.x-dev",
        "kongulov/interact-with-enum": "1.1.0",
        "laravel/fortify": "1.25.4",
        "laravel/framework": "12.7.2",
        "laravel/octane": "2.8.3",
        "laravel/sanctum": "4.0.8",
        "laravel/scout": "10.14.1",
        "laravel/tinker": "2.10.1",
        "laravel/wayfinder": "0.1.4",
        "league/flysystem-aws-s3-v3": "3.29.0",
        "meilisearch/meilisearch-php": "1.13.0",
        "pinkary-project/type-guard": "0.1.0",
        "spatie/enum": "3.13.0",
        "spatie/laravel-data": "4.14.1",
        "spatie/laravel-permission": "6.16.0",
        "spatie/laravel-prefixed-ids": "1.4.1",
        "spatie/laravel-query-builder": "6.3.1",
        "spatie/laravel-typescript-transformer": "2.5.1",
        "tightenco/ziggy": "2.5.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "3.5.5",
        "fakerphp/faker": "1.24.1",
        "larastan/larastan": "3.3.1",
        "laravel/pail": "1.2.2",
        "laravel/pint": "1.21.2",
        "laravel/sail": "1.41.0",
        "mockery/mockery": "1.6.12",
        "nunomaduro/collision": "8.8.0",
        "pestphp/pest": "3.8.1",
        "pestphp/pest-plugin-drift": "3.0.0",
        "pestphp/pest-plugin-laravel": "3.1.0",
        "pestphp/pest-plugin-type-coverage": "3.5.0",
        "rector/rector": "2.0.11"
    },
    "autoload": {
        "files": [
            "bootstrap/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "lint": "pint",
        "refactor": "rector",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run",
        "test:types": "phpstan analyse",
        "test": [
            "@test:lint",
            "@test:refactor",
            "@test:types"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
